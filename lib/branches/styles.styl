.imagen
	width 100%
	margin 0
	padding-top 20%
.logos
	width 82.5%
	margin 0 10%
.imagen:before
	content ""
	display block
	border-left solid 10px #ff6b00
	height 9.8%
	width 30%
	position absolute
	left 50%
	top 0
	bottom 0
	z-index: -1
