.zigzag1
	border 10px solid #ff6b00
	border-right none
	border-bottom none
	//transform rotate(59deg)
	transform matrix(1, -0.6, 0, 1, 0, 0)
	position absolute
	height 6%
	width 18%
	top 11.5%
	left 32%
	z-index -1
.zigzag2
	border 10px solid #ff6b00
	border-right none
	border-bottom none
	//transform rotate(59deg)
	transform matrix(1, -0.6, 0, 1, 0, 0)
	position absolute
	height 6.5%
	width 14%
	top 23.5%
	left 17.9%
	z-index -2
.zigzag3
	border-top 5px solid #ff6b00
	border-left 10px solid #ff6b00
	border-right none
	border-bottom none
	//transform rotate(59deg)
	transform matrix(1, -0.6, 1.5, 1, 0, 0)
	position absolute
	height 6%
	width 14%
	top 35%
	left 11.6%
	z-index -3
